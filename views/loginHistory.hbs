<div class="container">
  </br>
  <div class="row">
    <div class="col-md-12">
      </br>
      <h2>Login history</h2>
      <h3>User: {{session.user.username}}</h3>
      <hr/>
      <table class="table">
        <thead>
          <tr>
            <th scope="col">Date</th>
            <th scope="col">UserAgent</th>
          </tr>
        </thead>
        <tbody>
          {{#each session.user.loginHistory}}
          <tr>
            <td>{{dateTime}}</td>
            <td>{{userAgent}}</td>
          </tr>
          {{/each}}
        </tbody>
      </table>
    </div>
  </div>
</div>